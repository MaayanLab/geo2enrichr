{
	"name": "GEO2Enrichr",
	"short_name": "g2e",
	"version": "1.5",
	"description": "Ma'ayan Lab",
	"icons": {
		"128": "images/g2e-logo-128x128.png"
	},
	"permissions": [
		"downloads",
		"http://amp.pharm.mssm.edu/Enrichr/*"
	],
	"content_scripts": [
		{
			"matches": [ 
				"http://www.ncbi.nlm.nih.gov/sites/*",
				"http://www.ncbi.nlm.nih.gov/geo/query/acc.cgi*"
			],
			"css": ["main.css", "jquery-ui.min.css", "open-sans.css"],
			"js": ["jquery-1.11.1.min.js", "jquery-ui.min.js", "g2e.js"]
		}
	],
	"web_accessible_resources": [
		"images/*"
	],
	"manifest_version": 2,
	"content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}