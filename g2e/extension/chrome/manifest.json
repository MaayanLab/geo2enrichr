{
    "name": "GEO2Enrichr",
    "short_name": "g2e",
    "version": "4.7",
    "description": "Ma'ayan Lab",
    "icons": {
        "128": "logo-128x128.png"
    },
    "permissions": [
        "downloads",
        "https://amp.pharm.mssm.edu/Enrichr/"
    ],
    "background": {
        "scripts": ["g2e.background.js"],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [ 
                "https://www.ncbi.nlm.nih.gov/sites/*",
                "https://www.ncbi.nlm.nih.gov/geo/query/acc.cgi*",
                "https://amp.pharm.mssm.edu/gen3va/*"
            ],
            "css": ["open-sans.css", "main.css", "jquery-ui.min.css"],
            "js": ["jquery-1.11.2.min.js", "jquery-ui.min.js", "tag-it.min.js", "g2e.js"]
        }
    ],
    "web_accessible_resources": [
        "/*"
    ],
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
